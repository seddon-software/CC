include make.mk

all: subsystems
	
subsystems:
	@cd Binary\ Trees && $(MAKE)
	@cd BitFields && $(MAKE)
	@cd C\ Run\ Time && $(MAKE)
	@cd Database && $(MAKE)
	@cd Function\ Pointers && $(MAKE)
	@cd Function\ Prototypes && $(MAKE)
	@cd Preprocessor && $(MAKE)
	@cd Recursion && $(MAKE)
	@cd Searching && $(MAKE)
	@cd Sorting && $(MAKE)
	@cd Spreadsheet && $(MAKE)
	@cd Unicode && $(MAKE)
	

clean:
	@cd Binary\ Trees && $(MAKE) clean
	@cd BitFields && $(MAKE) clean
	@cd C\ Run\ Time && $(MAKE) clean
	@cd Database && $(MAKE) clean
	@cd Function\ Pointers && $(MAKE) clean
	@cd Function\ Prototypes && $(MAKE) clean
	@cd Preprocessor && $(MAKE) clean
	@cd Recursion && $(MAKE) clean
	@cd Searching && $(MAKE) clean
	@cd Sorting && $(MAKE) clean
	@cd Spreadsheet && $(MAKE) clean
	@cd Unicode && $(MAKE) clean
	@cd Variadic\ Functions && $(MAKE) clean


