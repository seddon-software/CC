set -v
# To use MTRACE, add a call to mtrace() at the begining of your C program
# (see myfile.c)

# compile
export MALLOC_CHECK_=3
gcc -g -Wall -pedantic myfile2.c -o myfile2.exe

# run the program
export MALLOC_TRACE=mtrace.txt
myfile2.exe
mtrace myfile2.exe mtrace.txt
cat mtrace.txt
mtrace mtrace.txt
ls -l
