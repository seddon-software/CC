{
  "version": "2.0.0",
  "configurations": [
    {
      "name": "Python",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "args": [],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "stopOnEntry": true
    },
    {
      "name": "make and build individual C file",
      "type": "cppdbg",
      "request": "launch",
      "program": "${fileDirname}/${fileBasenameNoExtension}.exe",
      "args": [],
      "stopAtEntry": true,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
          {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          }
      ],
      "preLaunchTask": "make",
      "miDebuggerPath": "/usr/bin/gdb",
  },
